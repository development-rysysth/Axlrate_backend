{
  "name": "axlrate-backend",
  "version": "1.0.0",
  "description": "Microservices backend with authentication",
  "main": "index.js",
  "scripts": {
    "build": "pnpm run build:shared && pnpm run build:gateway && pnpm run build:auth && pnpm run build:serpapi && pnpm run build:ingest && pnpm run build:aggregator && pnpm run build:export && pnpm run build:orchestrator",
    "build:shared": "pnpm --filter @axlrate/shared build",
    "build:gateway": "pnpm --filter @axlrate/api-gateway build",
    "build:auth": "pnpm --filter @axlrate/auth-service build",
    "build:serpapi": "pnpm --filter @axlrate/serpapi-service build",
    "build:ingest": "pnpm --filter @axlrate/ingest-service build",
    "build:aggregator": "pnpm --filter @axlrate/aggregator-service build",
    "build:export": "pnpm --filter @axlrate/export-service build",
    "build:orchestrator": "pnpm --filter @axlrate/scraper-orchestrator build",
    "start:gateway": "pnpm --filter @axlrate/api-gateway start",
    "start:auth": "pnpm --filter @axlrate/auth-service start",
    "start:serpapi": "pnpm --filter @axlrate/serpapi-service start",
    "start:ingest": "pnpm --filter @axlrate/ingest-service start",
    "start:aggregator": "pnpm --filter @axlrate/aggregator-service start",
    "start:export": "pnpm --filter @axlrate/export-service start",
    "start:orchestrator": "pnpm --filter @axlrate/scraper-orchestrator start",
    "dev:gateway": "pnpm --filter @axlrate/api-gateway dev",
    "dev:auth": "pnpm --filter @axlrate/auth-service dev",
    "dev:serpapi": "pnpm --filter @axlrate/serpapi-service dev",
    "dev:ingest": "pnpm --filter @axlrate/ingest-service dev",
    "dev:aggregator": "pnpm --filter @axlrate/aggregator-service dev",
    "dev:export": "pnpm --filter @axlrate/export-service dev",
    "dev:orchestrator": "pnpm --filter @axlrate/scraper-orchestrator dev",
    "dev": "concurrently \"pnpm run dev:gateway\" \"pnpm run dev:auth\" \"pnpm run dev:serpapi\" \"pnpm run dev:aggregator\"",
    "dev:all": "concurrently \"pnpm run dev:gateway\" \"pnpm run dev:auth\" \"pnpm run dev:serpapi\" \"pnpm run dev:ingest\" \"pnpm run dev:aggregator\" \"pnpm run dev:export\" \"pnpm run dev:orchestrator\"",
    "start": "concurrently \"pnpm run start:gateway\" \"pnpm run start:auth\" \"pnpm run start:serpapi\"",
    "start:all": "concurrently \"pnpm run start:gateway\" \"pnpm run start:auth\" \"pnpm run start:serpapi\" \"pnpm run start:ingest\" \"pnpm run start:aggregator\" \"pnpm run start:export\" \"pnpm run start:orchestrator\"",
    "install:all": "pnpm install",
    "migration:run": "npx --yes tsx scripts/run-migration.ts",
    "migration:005": "npx --yes tsx scripts/run-migration-005.ts",
    "migration:006": "npx --yes tsx scripts/run-migration-006.ts",
    "migration:007": "npx --yes tsx scripts/run-migration-007.ts",
    "migration:008": "npx --yes tsx scripts/run-migration-008.ts",
    "migration:009": "npx --yes tsx scripts/run-migration-009.ts",
    "migration:010": "npx --yes tsx scripts/run-migration-010.ts",
    "db:check": "npx --yes tsx scripts/check-database.ts",
    "update:hotels": "npx --yes tsx scripts/update-hotel-details.ts"
  },
  "keywords": [
    "microservices",
    "nodejs",
    "express",
    "authentication"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1"
  }
}
